generator client {
  provider = "prisma-client-js"
  // output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Color {
  id Int @id @default(autoincrement())
  label String @unique
  value String
  color String
}

model ColorBg{
  id Int @id @default(autoincrement())
  color String @unique
}
model StylesButtons{
  id Int @id @default(autoincrement())
  text String
  rounded String
}
